{
    "name": "lukebeer/broadworks-ocip",
    "description": "PHP ORM-like framework for the Broadworks OCIP API",
    "keywords": [
        "broadworks-oci",
        "broadworks-api",
        "php-framework",
        "broadworks",
        "broadsoft"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Luke Berezynskyj",
            "email": "eat.lemons@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "pear/http_request2": ">=2.2.1",
        "pear/console_table": ">=1.3.1"
    },
    "suggest": {
        "symfony/console": ">=2.7"
    },
    "autoload": {
        "psr-4": {
            "BroadworksOCIP\\": "src/BroadworksOCIP/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lukebeer/broadworks-ocip.git"
        },
        {
            "type": "pear",
            "url": "https://pear2.php.net"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform-check": false
    }
}
